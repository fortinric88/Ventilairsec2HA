  "services": [
    {
      "service": "set_boost",
      "description": "Set boost mode intensity",
      "fields": {
        "intensity": {"type": "integer", "min": 0, "max": 255, "description": "Boost intensity"}
      },
      "cmd_type": "MSC:1,command:0,BOOST:#slider#"
    },
    {
      "service": "set_vacances",
      "description": "Set vacation mode",
      "fields": {
        "value": {"type": "integer", "min": 0, "max": 1, "description": "Vacation mode on/off"}
      },
      "cmd_type": "MSC:1,command:0,VACS:#slider#"
    },
    {
      "service": "set_modefonc",
      "description": "Set operating mode",
      "fields": {
        "mode": {"type": "integer", "min": 0, "max": 3, "description": "Operating mode"}
      },
      "cmd_type": "MSC:1,command:0,MODEFONC:#slider#"
    },
    {
      "service": "set_surventilation",
      "description": "Set over-ventilation",
      "fields": {
        "value": {"type": "integer", "min": 0, "max": 1}
      },
      "cmd_type": "MSC:1,command:0,FONC:#message#",
      "note": "Constructs FONC message with surv flag"
    },
    {
      "service": "set_debit",
      "description": "Set fixed flow",
      "fields": {
        "value": {"type": "integer", "min": 0, "max": 1}
      },
      "cmd_type": "MSC:1,command:0,FONC:#message#",
      "note": "Constructs FONC message with debf flag"
    },
    {
      "service": "set_bypass",
      "description": "Set bypass mode",
      "fields": {
        "value": {"type": "integer", "min": 0, "max": 1}
      },
      "cmd_type": "MSC:1,command:0,FONC:#message#",
      "note": "Constructs FONC message with bypass flag"
    },
    {
      "service": "set_plage",
      "description": "Set time range",
      "fields": {
        "value": {"type": "integer", "min": 0, "max": 1}
      },
      "cmd_type": "MSC:1,command:0,FONC:#message#",
      "note": "Constructs FONC message with plage flag"
    },
    {
      "service": "reset_filter",
      "description": "Reset filter counter",
      "cmd_type": "MSC:1,command:0,COMMAND:#slider#"
    },
    {
      "service": "set_temp_preheating",
      "description": "Set preheating temperature",
      "fields": {
        "temp": {"type": "number", "min": 0, "max": 40, "description": "Temperature in °C"}
      },
      "cmd_type": "MSC:1,command:0,TEMPEL:#slider#"
    },
    {
      "service": "set_temp_supply",
      "description": "Set supply air temperature",
      "fields": {
        "temp": {"type": "number", "min": 0, "max": 50, "description": "Temperature in °C"}
      },
      "cmd_type": "MSC:1,command:0,TEMPSOUF:#slider#"
    },
    {
      "service": "set_temp_hydror",
      "description": "Set HydroR temperature setpoint",
      "fields": {
        "temp": {"type": "number", "min": 0, "max": 50, "description": "Temperature in °C"}
      },
      "cmd_type": "MSC:1,command:0,TEMPHYD:#slider#"
    },
    {
      "service": "set_temp_solar",
      "description": "Set solar comfort temperature",
      "fields": {
        "temp": {"type": "number", "min": 0, "max": 50, "description": "Temperature in °C"}
      },
      "cmd_type": "MSC:1,command:0,TEMPSOL:#slider#"
    },
    {
      "service": "send_hour",
      "description": "Send current time to VMI",
      "cmd_type": "MSC:1,command:1,HOUR:#message#"
    },
    {
      "service": "set_agenda",
      "description": "Set weekly agenda",
      "fields": {
        "agenda": {"type": "object", "description": "Agenda data structure"}
      },
      "cmd_type": "MSC:1,command:2,AGENDA:#message#"
    }
